{
  "name": "strava-tracker-backend",
  "version": "1.0.0",
  "description": "Strava activity tracker and analytics backend",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "lint": "tsc --noEmit",
    "build": "tsc",
    "test:import:parsers": "tsx --test src/tests/importParsers.test.ts",
    "test:import:watchfolder": "tsx --test src/tests/watchFolder.test.ts",
    "test:import:queue": "tsx --test src/tests/importQueue.test.ts",
    "test:segments": "tsx --test src/tests/localSegments.test.ts src/tests/segmentFilters.test.ts",
    "test:import": "tsx --test src/tests/importParsers.test.ts src/tests/watchFolder.test.ts src/tests/importQueue.test.ts src/tests/localSegments.test.ts src/tests/segmentFilters.test.ts",
    "collect": "tsx src/collector.ts",
    "db:init": "tsx src/scripts/initDb.ts",
    "db:migrate": "tsx src/scripts/migrateDb.ts",
    "db:check": "tsx src/scripts/checkDb.ts"
  },
  "keywords": [
    "strava",
    "analytics",
    "cycling",
    "running"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@cyclenoid/pwrx-adapter-strava": "github:cyclenoid/pwrx-adapter-strava#v0.2.0",
    "@types/compression": "^1.8.1",
    "adm-zip": "^0.5.16",
    "axios": "^1.6.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fit-file-parser": "^2.3.1",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.2.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
